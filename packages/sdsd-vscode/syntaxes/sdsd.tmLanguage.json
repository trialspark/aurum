{
  "$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
  "name": "study data schema definition",
  "patterns": [
    {
      "include": "#keywords"
    },
    {
      "include": "#strings"
    },
    {
      "include": "#tstrings"
    },
    {
      "include": "#property-names"
    }
  ],
  "repository": {
    "keywords": {
      "patterns": [
        {
          "name": "keyword.control.sdsd",
          "match": "\\b(study|milestone|interface|codelist|domain|dataset|extend|map)\\b"
        }
      ]
    },
    "property-names": {
      "patterns": [
        {
          "name": "support.type.property-name.json.sdsd",
          "match": "[a-zA-Z$_][a-zA-Z0-9$_]*"
        }
      ]
    },
    "tstrings": {
      "name": "string.quoted.double.t.sdsd",
      "begin": "t\"",
      "end": "\"",
      "patterns": [
        {
          "name": "keywork.operator.start.sdsd",
          "match": "t\""
        },
        {
          "name": "constant.other.character_class.regex.sdsd",
          "match": "d|m"
        },
        {
          "name": "keyword.operator.quantifier.sdsd",
          "match": "\\+|\\-"
        }
      ]
    },
    "strings": {
      "name": "string.quoted.double.sdsd",
      "begin": "\"",
      "end": "\"",
      "patterns": [
        {
          "name": "constant.character.escape.sdsd",
          "match": "\\\\."
        }
      ]
    }
  },
  "scopeName": "source.sdsd"
}
