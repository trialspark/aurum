{
  "$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
  "name": "study data schema definition",
  "patterns": [
    {
      "include": "#keywords"
    },
    {
      "include": "#strings"
    }
  ],
  "repository": {
    "keywords": {
      "patterns": [
        {
          "name": "keyword.control.sdsd",
          "match": "\\b(study|milestone|interface|codelist|domain|dataset|extend|map)\\b"
        }
      ]
    },
    "tstrings": {
      "name": "tstring.quoted.double.sdsd",
      "begin": "t\"",
      "end": "\"",
      "patterns": [
        {
          "name": "constant.character.escape.sdsd",
          "match": "\\\\."
        }
      ]
    },
	"strings": {
		"name": "string.quoted.double.sdsd",
		"begin": "\"",
		"end": "\"",
		"patterns": [
		  {
			"name": "constant.character.escape.sdsd",
			"match": "\\\\."
		  }
		]
	}
  },
  "scopeName": "source.sdsd"
}
